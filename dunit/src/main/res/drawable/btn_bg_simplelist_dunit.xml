<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape >
            <!-- 开始颜色="" 结束颜色="" 颜色渐变="" -->
            <gradient android:angle="270" android:endColor="@color/dunitColorButtonPressedEnd" android:startColor="@color/dunitColorButtonPressedStart" />
            <!-- 渐变方向 按钮边缘="" 边缘宽="" -->
            <stroke android:width="1sp" android:color="@color/dunitColorButtonPressedStroke" />
            <!-- 边缘颜色 按钮四个圆角="" -->
            <corners android:radius="30dp" />
            <!-- 半径 内边距="" 按钮文字和边缘距离="" -->
            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="@color/dunitColorButtonNormalEnd" android:startColor="@color/dunitColorButtonNormalStart" />
            <stroke android:width="1sp" android:color="@color/dunitColorButtonNormalStroke" />
            <corners android:radius="30dp" />
            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape>
    </item>

</selector>